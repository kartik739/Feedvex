{
  "name": "feedvex",
  "version": "1.0.0",
  "description": "Production-quality search engine for Reddit content",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && vite build",
    "build:all": "npm run build:backend && npm run build:frontend",
    "start": "node backend/dist/server.js",
    "dev": "ts-node backend/src/server.ts",
    "dev:backend": "ts-node backend/src/server.ts",
    "dev:frontend": "cd frontend && vite",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "seed": "ts-node backend/scripts/seed-test-data.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "lint": "eslint backend/src/**/*.ts frontend/src/**/*.{ts,tsx}",
    "lint:fix": "eslint backend/src/**/*.ts frontend/src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"backend/src/**/*.ts\" \"frontend/src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "search-engine",
    "reddit",
    "typescript",
    "bm25",
    "information-retrieval"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^7.2.2",
    "terser": "^5.46.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@types/natural": "^5.1.5",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.6",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "fast-check": "^3.15.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.575.0",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "opossum": "^9.0.0",
    "p-retry": "^7.1.1",
    "pg": "^8.18.0",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.28.0",
    "snoowrap": "^1.23.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "ws": "^8.16.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.2"
  }
}
