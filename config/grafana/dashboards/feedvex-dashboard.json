{
  "dashboard": {
    "title": "Feedvex Search Engine Metrics",
    "tags": ["feedvex", "search", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Query Latency (p95)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"feedvex-api\", route=\"/api/v1/search\"}[5m]))",
            "legendFormat": "p95 latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"feedvex-api\", route=\"/api/v1/search\"}[5m]))",
            "legendFormat": "p50 latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Throughput",
        "type": "graph",
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"feedvex-api\"}[5m])",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"feedvex-api\", status_code=~\"5..\"}[5m])",
            "legendFormat": "5xx errors",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"feedvex-api\", status_code=~\"4..\"}[5m])",
            "legendFormat": "4xx errors",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Errors/sec"
          }
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(cache_hits_total{job=\"feedvex-api\"}[5m]) / (rate(cache_hits_total{job=\"feedvex-api\"}[5m]) + rate(cache_misses_total{job=\"feedvex-api\"}[5m]))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Hit Rate",
            "max": 1,
            "min": 0
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Queries",
        "type": "stat",
        "gridPos": { "x": 0, "y": 16, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "queries_in_progress{job=\"feedvex-api\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Total Documents Indexed",
        "type": "stat",
        "gridPos": { "x": 6, "y": 16, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "index_documents_total{job=\"feedvex-api\"}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"feedvex-api\"}",
            "legendFormat": "RSS Memory",
            "refId": "A"
          },
          {
            "expr": "nodejs_heap_size_used_bytes{job=\"feedvex-api\"}",
            "legendFormat": "Heap Used",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          }
        ]
      },
      {
        "id": 8,
        "title": "Rate Limit Rejections",
        "type": "graph",
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(rate_limit_rejections_total{job=\"feedvex-api\"}[5m])",
            "legendFormat": "Rejections/sec",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
